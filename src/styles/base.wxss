page, 
view, 
text,
navigator,
swiper, 
swiper-item, 
scroll-view,
image, 
input{
	padding: 0;
	margin: 0;
	box-sizing: border-box;
}
page{
	/* 主题色 */
	--themeColor: #eb4450;
	/* 字体大小 */
	font-size: 28rpx;
	background-color: #007AFF;
}
image{
	width: 100%;
	display: block;
}
/* 主题色 */
.theme-color{
	color: var(--themeColor);
}
